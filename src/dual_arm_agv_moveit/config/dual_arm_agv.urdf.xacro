<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="dual_arm_agv">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import dual_arm_agv urdf file -->
    <xacro:include filename="$(find dual_arm_agv)/urdf/dual_arm_agv.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="dual_arm_agv.ros2_control.xacro" />


    <xacro:dual_arm_agv_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
